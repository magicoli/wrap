{
    "name": "magicoli/wrap-engine",
    "description": "WRAP Engine - Standalone media processing and file management library",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Olivier van Helden",
            "email": "olivier@van-helden.net",
            "homepage": "https://magiiic.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "php-ffmpeg/php-ffmpeg": "^0.19.0",
        "ralouphie/mimey": "^1.0",
        "blueimp/jquery-file-upload": "^10.32",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wrap\\": "./"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
